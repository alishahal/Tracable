<% if flash[:errors]%>
    <% flash[:errors].each do |e| %>
        <li style="color: red">
            <%= e %>
        </li>
    <%end%>
<%end%>

<%= form_with model:@user do |f|%>
    <%= f.label :username %>
    <%= f.text_field :username%>
    <br>
    <br>
    <%= f.label :name%>
    <%= f.text_field :name%>
    <br>
    <br>
    <%= f.label :email%>
    <%= f.text_field :email%>
    <br>
    <br>
    <%= f.label :image_url%>
    <%= f.text_area :image_url%>
    <br>
    <br>
    <%= f.submit%>
<% end %>